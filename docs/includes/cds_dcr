  </span><span class="s2">"tos_uri"</span><span class="p">:</span><span class="w"> </span><span class="s2">"https://adr.example.com/tos.html"</span><span class="p">,</span><span class="w">
  </span><span class="s2">"policy_uri"</span><span class="p">:</span><span class="w"> </span><span class="s2">"https://adr.example.com/policy.html"</span><span class="p">,</span><span class="w">
